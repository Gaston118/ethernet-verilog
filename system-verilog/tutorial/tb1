#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00FA3DD0 .scope module, "tb1" "tb1" 2 4;
 .timescale 0 0;
v010D9FF8_0 .var "a", 0 0;
v010DA050_0 .var "clk", 0 0;
v010DA0A8_0 .net "out1", 0 0, L_010DA158; 1 drivers
v010DA100_0 .net "out2", 0 0, L_010DA1B0; 1 drivers
S_00FA0038 .scope task, "toggle_clk" "toggle_clk" 2 48, 2 48, S_00FA3DD0;
 .timescale 0 0;
TD_tb1.toggle_clk ;
    %delay 10, 0;
    %load/v 8, v010DA050_0, 1;
    %inv 8, 1;
    %set/v v010DA050_0, 8, 1;
    %delay 10, 0;
    %load/v 8, v010DA050_0, 1;
    %inv 8, 1;
    %set/v v010DA050_0, 8, 1;
    %end;
S_010D9DE8 .scope module, "FSM" "fsm" 2 10, 3 10, S_00FA3DD0;
 .timescale 0 0;
P_010D9E74 .param/l "FINAL" 3 22, C4<100>;
P_010D9E88 .param/l "IDLE" 3 20, C4<001>;
P_010D9E9C .param/l "STATE_1" 3 21, C4<010>;
v010D3B60_0 .net *"_s0", 2 0, C4<010>; 1 drivers
v010D4A28_0 .net *"_s4", 2 0, C4<100>; 1 drivers
v010D4A80_0 .net "a", 0 0, v010D9FF8_0; 1 drivers
v010D4AD8_0 .net "clk", 0 0, v010DA050_0; 1 drivers
v010D3248_0 .alias "out1", 0 0, v010DA0A8_0;
v010D32A0_0 .alias "out2", 0 0, v010DA100_0;
v010D32F8_0 .var "state", 2 0;
E_010D82D8 .event posedge, v010D4AD8_0;
L_010DA158 .cmp/eq 3, v010D32F8_0, C4<010>;
L_010DA1B0 .cmp/eq 3, v010D32F8_0, C4<100>;
    .scope S_010D9DE8;
T_1 ;
    %wait E_010D82D8;
    %load/v 8, v010D32F8_0, 3;
    %cmpi/u 8, 1, 3;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 2, 3;
    %jmp/1 T_1.1, 6;
    %cmpi/u 8, 4, 3;
    %jmp/1 T_1.2, 6;
    %movi 8, 1, 3;
    %ix/load 0, 3, 0;
    %assign/v0 v010D32F8_0, 0, 8;
    %jmp T_1.4;
T_1.0 ;
    %load/v 8, v010D4A80_0, 1;
    %jmp/0xz  T_1.5, 8;
    %movi 8, 2, 3;
    %ix/load 0, 3, 0;
    %assign/v0 v010D32F8_0, 0, 8;
    %jmp T_1.6;
T_1.5 ;
    %movi 8, 1, 3;
    %ix/load 0, 3, 0;
    %assign/v0 v010D32F8_0, 0, 8;
T_1.6 ;
    %jmp T_1.4;
T_1.1 ;
    %load/v 8, v010D4A80_0, 1;
    %jmp/0xz  T_1.7, 8;
    %movi 8, 4, 3;
    %ix/load 0, 3, 0;
    %assign/v0 v010D32F8_0, 0, 8;
    %jmp T_1.8;
T_1.7 ;
    %movi 8, 1, 3;
    %ix/load 0, 3, 0;
    %assign/v0 v010D32F8_0, 0, 8;
T_1.8 ;
    %jmp T_1.4;
T_1.2 ;
    %load/v 8, v010D4A80_0, 1;
    %jmp/0xz  T_1.9, 8;
    %movi 8, 4, 3;
    %ix/load 0, 3, 0;
    %assign/v0 v010D32F8_0, 0, 8;
    %jmp T_1.10;
T_1.9 ;
    %movi 8, 1, 3;
    %ix/load 0, 3, 0;
    %assign/v0 v010D32F8_0, 0, 8;
T_1.10 ;
    %jmp T_1.4;
T_1.4 ;
    %jmp T_1;
    .thread T_1;
    .scope S_00FA3DD0;
T_2 ;
    %vpi_call 2 17 "$dumpfile", "dump.vcd";
    %vpi_call 2 18 "$dumpvars", 2'sb01, S_00FA3DD0;
    %set/v v010DA050_0, 0, 1;
    %set/v v010D9FF8_0, 0, 1;
    %vpi_call 2 22 "$display", "Initial out1: %0h, out2: %0h", v010DA0A8_0, v010DA100_0;
    %fork TD_tb1.toggle_clk, S_00FA0038;
    %join;
    %vpi_call 2 26 "$display", "IDLE out1: %0h, out2: %0h", v010DA0A8_0, v010DA100_0;
    %set/v v010D9FF8_0, 1, 1;
    %fork TD_tb1.toggle_clk, S_00FA0038;
    %join;
    %vpi_call 2 31 "$display", "STATE_1 out1: %0h, out2: %0h", v010DA0A8_0, v010DA100_0;
    %fork TD_tb1.toggle_clk, S_00FA0038;
    %join;
    %vpi_call 2 35 "$display", "FINAL out1: %0h, out2: %0h", v010DA0A8_0, v010DA100_0;
    %fork TD_tb1.toggle_clk, S_00FA0038;
    %join;
    %vpi_call 2 39 "$display", "FINAL out1: %0h, out2: %0h", v010DA0A8_0, v010DA100_0;
    %set/v v010D9FF8_0, 0, 1;
    %fork TD_tb1.toggle_clk, S_00FA0038;
    %join;
    %vpi_call 2 44 "$display", "IDLE out1: %0h, out2: %0h", v010DA0A8_0, v010DA100_0;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb1.sv";
    "./fsm.sv";
